@model IEnumerable<QlySukienSV.Models.SuKien>
@{
  ViewData["Title"] = "Danh sách sự kiện";
}
<h5>Danh sách sự kiện</h5>
<table class="table table-striped table-sm mt-3">
  <thead><tr><th>Tên</th><th>CLB</th><th>Thời gian</th><th>Địa điểm</th><th></th></tr></thead>
  <tbody>
  @foreach(var s in Model){
    <tr>
      <td>@s.TenSuKien</td>
      <td>@s.CLB?.TenCLB</td>
      <td>@s.NgayBatDau </td>
      <td>@s.DiaDiem</td>
      <td><a class="btn btn-sm btn-outline-primary" asp-controller="Home" asp-action="ChiTietSuKien" asp-route-id="@s.Id">Xem</a></td>
    </tr>
  }
  </tbody>
</table>
